<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="graphicsPane" minWidth="150.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.opentravel.dex.controllers.graphics.GraphicsCanvasController">
   <children>
      <VBox fx:id="graphicsVBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ToolBar prefHeight="40.0">
              <items>
                <Button mnemonicParsing="false" onAction="#doButton1" text="Clear" />
                  <Button layoutX="10.0" layoutY="12.0" mnemonicParsing="false" onAction="#doButton2" text="Animate" />
                  <Button layoutX="10.0" layoutY="12.0" mnemonicParsing="false" onAction="#doButton3" text="Refresh" />
                  <ColorPicker fx:id="colorPicker" onAction="#doColor" />
                  <Slider fx:id="slider" />
              </items>
            </ToolBar>
            <Pane fx:id="spritePane" prefHeight="600.0" prefWidth="400.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
