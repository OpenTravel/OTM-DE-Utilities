<Tab fx:id="searchTab" text="Search" xmlns="http://javafx.com/javafx/9.0.1" 
		xmlns:fx="http://javafx.com/fxml/1" 
		fx:controller="org.opentravel.dex.controllers.search.SearchTabController">
     <content>
        <VBox fx:id="searchTabVBox">
           <children>
              <fx:include fx:id="repositorySelection" source="../RepositoryViews/RepositorySelection.fxml" />
              <fx:include fx:id="searchQuery" source="SearchQuery.fxml" />
              <fx:include fx:id="searchResults" source="SearchResults.fxml" VBox.vgrow="ALWAYS" />
           </children>
        </VBox>
     </content>
</Tab>
